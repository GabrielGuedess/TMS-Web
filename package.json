{
  "name": "tms-wolves",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "postbuild": "next-sitemap",
    "build-storybook": "storybook build",
    "build:analyzer": "ANALYZE=true next build",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "codegen": "graphql-codegen --config codegen.ts",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "find-deadcode": "ts-prune -p tsconfig.json -e",
    "generate": "pnpm plop --plopfile generators/plopfile.mjs",
    "generate:atom": "pnpm plop --plopfile ./generators/plopfile.mjs atoms",
    "generate:mole": "pnpm plop --plopfile ./generators/plopfile.mjs molecules",
    "generate:orgs": "pnpm plop --plopfile ./generators/plopfile.mjs organisms",
    "postinstall": "husky install",
    "lint": "eslint src --max-warnings=0 -f mo",
    "lint:fix": "pnpm eslint . --fix",
    "lint:interactive": "eslint-nibble \"{src,apps,libs,test}/**/*.ts\"",
    "release": "semantic-release",
    "sort:package": "sort-package-json",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "test:watch": "jest --watch --maxWorkers=25%",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "package.json": "pnpm sort:package",
    "src/**/*.{ts,tsx}": [
      "bash -c tsc --noEmit",
      "pnpm find-deadcode",
      "pnpm lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
